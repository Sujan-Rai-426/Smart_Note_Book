{
    "version": 2,
    "builds": [
      {
        "src": "Frontend_react/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "Backend_django/manage.py",
        "use": "@vercel/python"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/Backend_django/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/Frontend_react/index.html"
      }
    ],
    "static": {
      "public": "/Frontend_react/build"
    }
  }
  