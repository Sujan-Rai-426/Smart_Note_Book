

{
    "version": 2,
    "builds": [
        {
            "src": "Frontend_react/package.json",
            "use": "@vercel/static-build" 
        },
        {
            "src": "Backend_django/manage.py",
            "use": "@vercel/python"  
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Frontend_react/$1"
            },
        {
            "src": "/api/(.*)",
            "dest": "Backend_django/$1"
        }
    ]
}
